#include <iostream>
#include <fstream>

using namespace std;

int a,b;
ofstream g("rez2.txt");
void citire()
{
    cout<<"a=";
    cin>>a;
    cout<<"b=";
    cin>>b;
}
void rezultat()
{
    g<<"Suma este:"<<a+b<<endl;
    g<<"Produsul este:"<<a*b<<endl;
}
int main()
{
    citire();
    rezultat();
    return 0;
}
