include <iostream>
#include <math.h>

using namespace std;

//VECTORI--FISA DE LUCRU

//4.

unsigned int i,n;
int a[100];
void citire()
{
    cout<<"n=";
    cin>>n;
    for(i=0;i<n;i++)
    {
        cout<<"a["<<i<<"]=";
        cin>>a[i];
    }
}
int numere_prime(int x)
{
    int d;
    for(d=2;d<=(x/2);d++)
    {
        if(x%d==0)
            return 0;
    }
    return 1;
}
void afisare1()
{
    unsigned int nr=0;
    for(i=0;i<n;i++)
    {
        if(a[i]>=0&&a[i]<=9&&numere_prime(a[1])==1)
        {
            cout<<a[i]*a[i]<<" ";
            nr++;
            if(nr%5==0)
                cout<<endl;
        }
    }
}
int suma_cifrelor(int x)
{
    int s=0;
    while(x!=0)
    {
        if((x%10)%3==0)
            s+=(x%10);
        x/=10;
    }
    return s;
}
void afisare2()
{
    unsigned int nr=0;
    for(i=0;i<n;i++)
    {
        if(a[i]>=1000&&a[i]<=9999)
            {
                cout<<suma_cifrelor(a[i])<<" ";
                nr++;
                if(nr%5==0)
                    cout<<endl;
            }
    }
}
int suma_cifrelor2(int x)
{
    int s=0;
    while(x!=0)
    {
        s+=(x%10);
        x/=10;
    }
    return s;
}
void afisare3()
{
    unsigned int nr=0;
    for(i=0;i<n;i++)
    {
        if(a[i]>=10&&a[i]<=99&&numere_prime(suma_cifrelor2(a[i]))==1)
        {
            cout<<a[i]<<" ";
            nr++;
            if(nr%5==0)
                cout<<endl;
        }
    }
}
int suma_divizori_impari(int x)
{
    int d,s=0;
    for(d=1;d<=x;d+=2)
    {
        if(x%d==0)
            s+=d;
    }
    return s;
}
void afisare4()
{
    unsigned int nr=0;
    for(i=0;i<n;i++)
    {
            if(a[i]>=10&&a[i]<=99)
            {
                cout<<suma_divizori_impari(a[i])<<" ";
                nr++;
                if(nr%5==0)
                    cout<<endl;
            }
    }
}
int inversul(int x)
{
    int inv=0;
    while(x!=0)
    {
        inv=inv*10+(x%10);
        x/=10;
    }
    return inv;
}
void afisare5()
{
    unsigned int nr=0;
    for(i=0;i<n;i++)
    {
        if(a[i]>=10&&sqrt(a[i])==(int)sqrt(a[i])&&sqrt(inversul(a[i]))==(int)sqrt(inversul(a[i])))
        {
            cout<<a[i]<<" ";
            nr++;
            if(nr%5==0)
                cout<<endl;
        }
    }
}
int numarul_zerouri(int x)
{
    unsigned int nr=0;
    while(x!=0)
    {
        if((x%10)==0)
            nr++;
        x/=10;
    }
    return nr;
}
void afisare6()
{
    unsigned int nr=0;
    for(i=0;i<n;i++)
    {
        if(a[i]>=10)
        {
            cout<<numarul_zerouri(a[i])<<" ";
            nr++;
            if(nr%5==0)
                cout<<endl;
        }
    }
}
int main()
{
    citire();
    afisare1();
    cout<<endl;
    cout<<endl;
    afisare2();
    cout<<endl;
    cout<<endl;
    afisare3();
    cout<<endl;
    cout<<endl;
    afisare4();
    cout<<endl;
    cout<<endl;
    afisare5();
    cout<<endl;
    cout<<endl;
    afisare6();
    return 0;
}
